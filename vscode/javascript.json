{	
	"sample": {
		"prefix": "sam",
		"body":[
			"/**",
			" * $1",
			" */",
			"\"use strict\"",
			"",
			"function main(args) {",
			"",
			"}",
			"",
			"process.stdin.resume();",
			"process.stdin.setEncoding('utf-8');",
			"var stdin_input = \"\";",
			"",
			"process.stdin.on(\"data\", function(input) {",
			" stdin_input += input;",
			"});",
			"",
			"process.stdin.on(\"end\", function() {",
			" main(stdin_input.split(\"\\n\"));",
			"});"
		]
	},

	"file input": {
		"prefix": "fi",
		"body":[	
			"var fs = require('fs');",
			"var data = fs.readFileSync('input.txt', 'utf-8');",
			"main(data.split(\"\\n\"));"
		]
	},

